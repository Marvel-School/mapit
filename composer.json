{
    "name": "mapit/application",
    "description": "A travel destination mapping and tracking platform",
    "type": "project",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "seed": "php scripts/DatabaseSeeder.php"
    },
    "authors": [
        {
            "name": "MapIt Developer",
            "email": "developer@mapit.com"
        }
    ]
}